{
  "name": "@pkgjs/support",
  "version": "0.0.2",
  "description": "Package support information (see: https://github.com/nodejs/package-maintenance/pull/220)",
  "author": "Wes Todd <wes@wesleytodd.com>",
  "keywords": [
    "support",
    "pacakge maintenence",
    "sustainability"
  ],
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pkgjs/support.git"
  },
  "bin": {
    "support": "./bin/support"
  },
  "support": {
    "versions": [
      {
        "version": "*",
        "target": {
          "node": "maintained"
        },
        "response": {
          "type": "best-effort"
        },
        "backing": {
          "hobby": "https://github.com/pkgjs/support"
        }
      }
    ]
  },
  "scripts": {
    "test": "standard && mocha --delay",
    "prepublushOnly": "npm t",
    "postpublish": "git push origin && git push origin --tags"
  },
  "dependencies": {
    "ajv": "^6.11.0",
    "loggerr": "^3.0.0-0",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "fs-extra": "^8.1.0",
    "mocha": "^6.2.0",
    "standard": "^14.0.2"
  }
}
